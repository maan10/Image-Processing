   "source": [
    "from skimage import data\n",
    "import cv2 as cv\n",
    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "id": "38475e17",
   "metadata": {},
   "outputs": [],
   "source": [
    "image=data.astronaut()\n",
    "gray_scale=cv.cvtColor(image,cv.COLOR_BGR2BGRA)\n",
    "canny=cv.Canny(gray_scale,50,240)\n",
    "cv.imshow(\"image\",gray_scale)\n",
    "cv.imshow(\"Canny\",canny)\n",
    "\n",
    "cv.waitKey()\n",
    "cv.destroyAllWindows()\n"
   ]
  }
